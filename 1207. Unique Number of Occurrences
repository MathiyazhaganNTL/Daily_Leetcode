class Solution(object):
    def uniqueOccurrences(self, arr):
        cnt=[]
        val=set()
        for item in arr :
            if item not in val  :
                new=arr.count(item)
                cnt.append(new)
                val.add(item)
        # print(cnt)
        find=[]
        for item in cnt :
            if item not in find :
                find.append(item)
            else :
                return False
        return True
    

        
