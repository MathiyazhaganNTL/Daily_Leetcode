class Solution {
    public static void swap(int[]arr,int s, int e){
        int temp=arr[s];
        arr[s]=arr[e];
        arr[e]=temp;
    }
    public int[] numberGame(int[] arr) {
        Arrays.sort(arr);
        
        for(int i=0;i<arr.length;i=i+2){swap(arr,i,i+1);}

        return arr;
        
    }
}
