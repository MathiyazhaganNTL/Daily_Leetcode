class Solution(object):
    def countTriples(self, n):
        """
        :type n: int
        :rtype: int
        """
        count = 0
        for i in range(1, n+1):         
            for j in range(1, n+1):   
                for k in range(1, n+1): 
                    a = k * k
                    b = j * j
                    c = i * i           
                    if a + b == c:      
                        count += 1
        return count
