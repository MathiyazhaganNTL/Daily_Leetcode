class Solution(object):
    def maxFreqSum(self, s):
        """
        :type s: str
        :rtype: int
        """
        v=set()
        c=set()
        vowels="aeiou"
        vcnt=0
        ccnt=0
        for i in range(len(s)) :
            if s[i]  in vowels and s[i] not in v:
                v.add(s[i])
                val=s.count(s[i])
                vcnt=max(vcnt,val)
            elif s[i] not in c and s[i] not in v :
                    c.add(s[i])
                    val1=s.count(s[i])
                    ccnt=max(ccnt,val1)
        return vcnt+ccnt
