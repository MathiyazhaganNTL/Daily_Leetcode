class Solution(object):
    def dayOfYear(self, date):
        """
        :type date: str
        :rtype: int
        """
        month = int(date[5:7])
        days = int(date[8:10])
        year = int(date[0:4])
        totaldays = 0
        dic = {1:31, 2:29, 3:31, 4:30, 5:31, 6:30, 
            7:31, 8:31, 9:30, 10:31, 11:30, 12:31}
        leap = False
        if (year % 4 == 0 and year % 100 != 0) or (year % 400 == 0):
            leap = True
        if not leap:
            dic[2] = 28
        for i in range(1, month):
            totaldays += dic[i]
        totaldays += days
        return totaldays
        
