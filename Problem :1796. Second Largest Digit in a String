class Solution(object):
    def secondHighest(self, s):
        """
        :type s: str
        :rtype: int
        """
        digits = set()
        for item in s:
            if item.isdigit():
                digits.add(int(item))
        
        digits = sorted(digits)
        if len(digits) < 2:
            return -1
        return digits[-2] 
