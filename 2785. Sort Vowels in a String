class Solution(object):
    def sortVowels(self, s):
        """
        :type s: str
        :rtype: str
        """
        v="aeiouAEIOU"
        vowels=[]
        for i in range(len(s)) :
            if s[i] in v :
                vowels.append(s[i])
        vowels.sort()
        cnt=0
        new=""
        for i in range(len(s)) :
            if s[i] in v :
                new+=vowels[cnt]
                cnt+=1
            else :
                new+=s[i]
        return new

                
