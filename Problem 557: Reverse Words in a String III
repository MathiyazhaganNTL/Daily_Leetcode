class Solution(object):
    def reverseWords(self, s):
        """
        :type s: str
        :rtype: str
        """
        li=[]
        sub=""
        for item in s  :
            if item == " " :
                li.append(sub[::-1])
                sub=""
            else :
                sub+=item
        li.append(sub[::-1])
        return " ".join(li)
