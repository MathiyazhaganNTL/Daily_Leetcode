class Solution(object):
    def lastStoneWeight(self, stones):
        """
        :type stones: List[int]
        :rtype: int
        """
        def local(arr):
            arr.sort()
            n = len(arr)
            if n == 1:
                return arr
            y = arr[-1]
            x = arr[-2]
            if x == y:
                new_arr = arr[:-2]
            else:
                new_arr = arr[:-2]
                new_arr.append(y - x)
            return new_arr
        
        while len(stones) > 1:
            stones = local(stones)
        
        return stones[0] if stones else 0
