class Solution:
    def reverseVowels(self, s: str) -> str:
        v="aeiouAEIOU"
        li=[]
        new=""
        for i in range(len(s)) :
            if s[i] in v :
                li.append(i)
        li.reverse()
        cnt=0
        for i in range(len(s)) :
            if s[i] in v and cnt<len(li) :
                new+=s[li[cnt]]
                cnt+=1
            else :
                new+=s[i]
        return new

        
