int* searchRange(int* nums, int numsSize, int target, int* returnSize) {
int *value = (int*)malloc(2 * sizeof(int));
    value[0]=-1;
     value[1]=-1;
for(int i=0 ;i<numsSize;i++)
{
    if(nums[i]==target)
    {
        if(value[0]==-1)
        {
            value[0]=i;
        }
        
        {
            value[1]=i;
        }
    }
}
*returnSize=2;
return value;
    
}
