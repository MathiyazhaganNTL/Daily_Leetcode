class Solution(object):
    def reformatDate(self, date):
        """
        :type date: str
        :rtype: str
        """
        
        dic = {"Jan": 1, "Feb": 2, "Mar": 3, "Apr": 4, "May": 5, "Jun": 6,
               "Jul": 7, "Aug": 8, "Sep": 9, "Oct": 10, "Nov": 11, "Dec": 12}

        val = date[0:2]
        if val.isdigit():
            val = int(val)
            day = val
            month = date[5:8]
            year = date[9:13]
        else:
            day = int(date[0:1])
            month = date[4:7]
            year = date[8:12]

        if day < 10:
            day = "0" + str(day)
        else:
            day = str(day)

        month = dic[month]
        if month < 10:
            month = "0" + str(month)
        else:
            month = str(month)

        result = year + "-" + month + "-" + day
        return result
