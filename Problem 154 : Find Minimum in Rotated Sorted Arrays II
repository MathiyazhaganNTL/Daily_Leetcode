class Solution {
    public int findMin(int[] nums) {
        // Arrays.sort(nums);
        // return nums[0];
        int value=Integer.MAX_VALUE;
        for(int i=0;i<nums.length;i++)
        {
            if(value>nums[i])
            {
                value=nums[i];
            }
        }
        return value;
    }
}
