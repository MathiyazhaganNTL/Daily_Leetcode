class Solution(object):
    def hasSameDigits(self, s):
        """
        :type s: str
        :rtype: bool
        """
        while len(s) > 2:
            find = ""
            n = len(s)
            for i in range(n - 1):
                new = (int(s[i]) + int(s[i + 1])) % 10
                find += str(new)
            s = find  
        if len(s) == 2 and s[0] == s[1]:
            return True
        return False
