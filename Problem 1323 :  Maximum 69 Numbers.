class Solution {
    public int maximum69Number(int num) {
        int max = num; 
        String dub = Integer.toString(num);
        int length = dub.length();

        for (int i = 0; i < length; i++) {
            char[] chars = dub.toCharArray(); 
            if (chars[i] == '6') {
                chars[i] = '9';
            } else if (chars[i] == '9') {
                chars[i] = '6';
            }
            int value = Integer.parseInt(new String(chars));
            max = Math.max(max, value);
        }

        return max;
    }
}
