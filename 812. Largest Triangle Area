class Solution(object):
    def largestTriangleArea(self, points):
        """
        :type points: List[List[int]]
        :rtype: float
        """
        row = len(points)
        result = 0.0
        for i in range(row):
            for j in range(i+1, row):
                for k in range(j+1, row):
                    x1, y1 = points[i]
                    x2, y2 = points[j]
                    x3, y3 = points[k]
                    val = 0.5 * abs(x1*(y2-y3) + x2*(y3-y1) + x3*(y1-y2))
                    result = max(result, val)
        return result
                
