class Solution:
    def areaOfMaxDiagonal(self, dimensions: List[List[int]]) -> int:
                #diagonal length and area
        max_ = (-1,-1)


        for l,w in dimensions:
            diagonal_length_squared = l**2 + w**2
            area = l*w

            max_ = max(max_, (diagonal_length_squared, area))
        
        return max_[-1]
