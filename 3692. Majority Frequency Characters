class Solution:
    def majorityFrequencyGroup(self, s: str) -> str:
        freq = Counter(s)
        d = {}
        f = {}
        for key, val in freq.items():
            if val in d:
                d[val] += 1
                f[val] += key
            else:
                d[val] = 1
                f[val] = key
        m = 0
        k = 0
        for key, val in d.items():
            if val > m:
                k = key
                m = val
                ans = f[key]
            elif val == m and key > k:
                k = key
                m = val
                ans = f[key]
        return ans
            
        
